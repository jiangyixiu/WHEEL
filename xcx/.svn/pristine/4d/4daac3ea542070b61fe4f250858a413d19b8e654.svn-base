/* pages/Matchinfo/Matchinfo.wxss */
@import "../../wxParse/wxParse.wxss";
.wrap{font-family: '微软雅黑','Microsoft YaHei';background: #f6f6f6;padding-bottom: 150rpx;min-height: 100%;}
.banner{height: 350rpx;position: relative;background: #ffffff;}
.slide-image{width: 100%;}
.title{position: absolute;bottom: 0;left: 0;width: 100%;height: 95rpx;background: rgba(0, 0, 0, 0.5);box-sizing: border-box;color: #ffffff;}
.title_img image{width: 70rpx;height: 70rpx;margin: 12rpx 0 0 12rpx;}
.title_text{flex: 1;overflow: hidden;}
.china{font-size: 34rpx;line-height: 34rpx;margin-top: 14rpx;margin-left: 12rpx;}
.china text,eng text{text-overflow: ellipsis;white-space: nowrap;width: 100%;overflow: hidden;display: block;}
.china2{line-height: 95rpx;padding-left: 30rpx;font-size: 34rpx;}
.china2 text{text-overflow: ellipsis;white-space: nowrap;width: 100%;overflow: hidden;display: block;}
.eng{font-size: 24rpx;line-height: 24rpx;margin-top: 8rpx;margin-left: 12rpx;}
.image_percent{height: 95rpx;width: 95rpx;position: relative;font-size: 22rpx;}
.line_dip{width: 38rpx;height: 2rpx;background: #ffffff;transform: rotateZ(-60deg);position: absolute;left: 26rpx;top: 51rpx;}
.part{position: absolute;top: 25rpx;left: 26rpx;}
.all{position: absolute;bottom: 18rpx;right: 30rpx;}
.indicator{text-align: center;height: 10rpx;}
.indicator view{width: 10rpx;height: 10rpx;border-radius: 50%;background: #efeef4;opacity: 0.45;margin-left: 10rpx;}
/*////////////赛事信息*/
.match_info{height: 109rpx;border-bottom: 2rpx solid #efefef;overflow: hidden;box-sizing: border-box;color: #333333;font-size: 30rpx;background: #ffffff;}
.match_type{height: 109rpx;line-height: 109rpx;width: 260rpx;overflow: hidden;border-right: 1px solid #efefef;}
.match_type image,.match_location image{margin-top: 35rpx;margin-left: 30rpx;margin-right: 20rpx;}
.match_location{flex: 1;overflow: hidden;box-sizing: border-box;position: relative;}
.mask_match_location{position: absolute;width: 72rpx;height: 100%;top: 0;right: 0;background: #ffffff;opacity: 0.8;}
/*/////////////赛事时间/////////////////*/
.match_time{height: 190rpx;box-sizing: border-box;padding:  0 14rpx;background: #ffffff;}
.match_time_image{width: 27rpx;height: 190rpx;}
.start_time{flex: 1;height: 190rpx;}
.start_time_image{height: 52rpx;width: 52rpx;margin: 0 auto;background: url(http://weixin.zx-tour.com/static/xcx_img/time.png) no-repeat;background-size: 458rpx;background-position: -28rpx -29rpx;margin-top: 38rpx;}
.stop_time_image{height: 52rpx;width: 52rpx;margin: 0 auto;background: url(http://weixin.zx-tour.com/static/xcx_img/time.png) no-repeat;background-size: 458rpx;background-position: -168rpx -29rpx;margin-top: 38rpx;}
.kaisai_time_image{height: 52rpx;width: 52rpx;margin: 0 auto;background: url(http://weixin.zx-tour.com/static/xcx_img/time.png) no-repeat;background-size: 458rpx;background-position: -308rpx -29rpx;margin-top: 38rpx;}
.start_time_text{font-size: 26rpx;color: #b2b2b2;text-align: center;line-height: 26rpx;margin-top: 12rpx;}
.start_day{font-size: 22rpx;color: #b2b2b2;text-align: center;line-height: 22rpx;margin-top: 6rpx;}
.blue{color: #1b9afc;}
.blue_start_image{background: url(http://weixin.zx-tour.com/static/xcx_img/time.png) no-repeat;background-position: -98rpx -29rpx;background-size: 458rpx;}
.blue_stop_image{background: url(http://weixin.zx-tour.com/static/xcx_img/time.png) no-repeat;background-position: -238rpx -29rpx;background-size: 458rpx;}
.blue_kaisai_image{background: url(http://weixin.zx-tour.com/static/xcx_img/time.png) no-repeat;background-position: -378rpx -29rpx;background-size: 458rpx;}
/*///////////////底部导航//////////////////////*/
.footer{position: fixed;bottom: -1rpx;left: 0; height: 98rpx;line-height: 97rpx;width: 100%;background: #ffffff;box-sizing: border-box;overflow: hidden;border-top: none;}
.match_list{flex: 1;border-top: 0.5rpx solid #d9d9d9;box-sizing: border-box;}
.remind{flex: 1;border-top: 0.5rpx solid #d9d9d9;box-sizing: border-box;}
.kefu{flex: 1;border-top: 0.5rpx solid #d9d9d9;box-sizing: border-box;}
.btn{width: 349rpx;height: 100%;background: #1b9afc;color: #ffffff;text-align: center;font-size: 36rpx;font-weight: bold;}
.footer_text{font-size: 22rpx;color: #888888;line-height: 22rpx;text-align: center;margin-top: 9rpx;}
.match_list_image{background: url(http://weixin.zx-tour.com/static/xcx_img/match_list.png) no-repeat;background-size: 100%;width: 46rpx;height: 41rpx;margin: 15rpx auto 0rpx;}
.match_remind_image{background: url(http://weixin.zx-tour.com/static/xcx_img/remind.png) no-repeat;background-size: 100%;width: 36rpx;height: 40rpx;margin: 14rpx auto 0rpx;}
.match_kefu_image{background: url(http://weixin.zx-tour.com/static/xcx_img/kefu.png) no-repeat;background-size: 100%;width: 38rpx;height: 39rpx;margin: 14rpx auto 0rpx;}
/*/////////////套餐选择介绍标题///////*/
.content_title{position: relative;font-size: 34rpx;color: #434343;padding-top: 4rpx; background: #ffffff;margin-top: 12rpx;overflow: hidden;}
.discript_title{flex: 1;text-align: center;height: 84rpx;line-height: 84rpx;}
.meal_title{flex: 1;text-align: center;height: 84rpx;line-height: 84rpx;}
.content_line_1{width: 378rpx;height: 4rpx;background: #0398ff;position: absolute;top: 0rpx;left: -1rpx;transition: left 0.3s linear;overflow: scroll!important;}
.bottom_line{border-bottom: 2rpx solid #d9d9d9;}
.trs_form{left: 375rpx;}
/*////////////////套餐部分//////////////*/
.city_box{background: #ffffff;padding-bottom: 24rpx;padding: 43rpx 28rpx 24rpx;overflow: hidden;border-bottom: 2rpx solid #efeef3;}
.go_city_title{height: 50rpx;line-height: 50rpx;color: #434343;font-size: 28rpx;margin-right: 19rpx;float: left;width: 156rpx;text-align: center;}
.city{height: 60rpx;width: 156rpx;box-sizing: border-box;border-radius: 10rpx;background: #f8f8f8;margin-bottom: 19rpx;line-height: 60rpx;font-size: 26rpx;text-align: center;position: relative;margin-right: 19rpx;border: 2rpx solid none;float: left;}
.city_select{position: absolute;width: 100%;height: 100%;top: 0;left: 0;}
.city_curr{background: #e4f4ff;border-color: #0398ff;color: #0398ff;}
.city_box>view:nth-child(4n){margin-right: 0;}
/*////////////////////////套餐信息样式////////////////*/
.meal_box{box-sizing: border-box;background: #ffffff;}
.conmon{width: 100%;box-sizing: border-box;}
.conmon_title{height: 114rpx;line-height: 114rpx;padding: 0 28rpx;border-top: 2rpx solid #efeef3;}
.conmon_title_tag{width: 28rpx;height: 28rpx;text-align: center;line-height: 28rpx;font-size: 18rpx;background: #0398ff;border-radius: 50%;color: #ffffff;margin-top: 43rpx;margin-right: 11rpx;}
.conmon_title_tag_grey{width: 28rpx;height: 28rpx;text-align: center;line-height: 28rpx;font-size: 18rpx;background: #bbbbbb;border-radius: 50%;color: #ffffff;margin-top: 43rpx;margin-right: 11rpx;}
.conmon_title_text{font-size: 34rpx;font-weight: bold;color: #3e3e3e;flex: 1;box-sizing: border-box;}
.conmon_title_price{width: 152rpx;font-size: 24rpx;box-sizing: border-box;margin-top: 18rpx;}
.conmon_title_price_top,.conmon_title_price_top_grey{font-weight: bold;color: #0398ff;border: 1px solid #e4e4e4;border-bottom: none;height: 40rpx;line-height: 40rpx;text-align: center;border-radius: 10rpx 10rpx 0 0;}
.conmon_title_price_top_grey{color: #bbbbbb;}
.conmon_title_price_bottom{height: 34rpx;line-height: 34rpx;text-align: center;color: #ffffff;font-size: 22rpx;background: #0398ff;border-radius: 0 0 10rpx 10rpx;}
.conmon_title__icon{width: 24rpx;height: 13rpx;background: url(http://weixin.zx-tour.com/static/xcx_img/taocan_icon.jpg) no-repeat;background-size: 100%;margin-left: 15rpx;margin-top: 54rpx;}
.conmon_info{padding: 35rpx 28rpx 0;}
.room{font-size: 28rpx;}
.is_room{width: 104rpx;height: 48rpx;text-align: center;line-height: 48rpx;border: 2rpx solid #0398ff;border-radius: 10rpx;color: #0398ff;margin-right: 20rpx;}
.room_price view:nth-child(1){font-size: 24rpx;margin-top: 10rpx;}
.room_price view:nth-child(2){font-size: 34rpx;}
.room_price view:nth-child(3){font-size: 24rpx;margin-top: 10rpx;}
.conmon_course{width: 345rpx;height: 60rpx;line-height: 60rpx;text-align: center;background: #f8f8f8;font-size: 28rpx;margin-bottom: 4rpx;}
.course_line{width: 8rpx;height: 60rpx;}
/*///////////////////标签样式*/
.tip{position: relative;overflow: hidden;margin-right: 22rpx;margin-top: 7rpx;}
.tip view:nth-child(1) {
    border-top: 20rpx solid rgba(0,0,0,0);
    width: 0px;
    height: 0px;
    border-bottom: 20rpx solid rgba(0,0,0,0);
    border-right: 18rpx solid #ff9320;
    float: left;
}
.tip view:nth-child(2) {
    float: left;
    background: #ff9320;
    color: #FFFFFF;
    line-height: 40rpx;
    padding: 0 6rpx;
    font-size: 24rpx;
}
.tip view:nth-child(3) {
    position: absolute;
    width: 7rpx;
    height: 7rpx;
    background: #FFFFFF;
    top: 16rpx;
    left: 12rpx;
    border-radius: 50%;
}
.room_explan view:nth-child(1){color: #3e3e3e;font-size: 28rpx;line-height: 28rpx;padding-bottom: 28rpx;margin-top: 18rpx;}
.room_explan view:nth-child(2){color: #3e3e3e;font-size: 28rpx;line-height: 28rpx;margin-bottom: 24rpx;}
.price_explan_img{height: 42rpx;width: 682rpx;background: url(http://weixin.zx-tour.com/static/xcx_img/pay_explane.jpg)no-repeat;background-size: 100%;}
/*/////////////////参考行程//////////////////////*/
.reference_march{height: 66rpx;width: 100%;border: 2rpx solid #0398ff;line-height: 66rpx;background: #e4f4ff;border-radius: 10rpx;margin-top: 40rpx;}
.reference_march view:nth-child(1){font-size: 30rpx;color: #0398ff;padding-left: 18rpx;flex: 1;}
.reference_march view:nth-child(2){width: 170rpx;height: 32rpx;text-align: center;font-size: 24rpx;border: 2rpx solid #0398ff;margin-top: 15rpx;line-height: 32rpx;color: #0398ff;border-radius: 16rpx;background: #ffffff;overflow: hidden;}
.reference_march view:nth-child(3){width: 13rpx;height: 24rpx;background: url(http://weixin.zx-tour.com/static/xcx_img/march_icon.png)no-repeat;background-size: 100%;margin: 22rpx 21rpx 0 15rpx;}
.end{width: 274rpx;height: 39rpx;background: url(http://weixin.zx-tour.com/static/xcx_img/end_icon.png)no-repeat;background-size: 100%;margin: 26rpx auto 23rpx;}
.introduce{background: #FFFFFF;box-sizing: border-box;padding: 0 28rpx 28rpx;}
.introduce_conmon{width: 236rpx;height: 94rpx;background: url(http://weixin.zx-tour.com/static/xcx_img/introduce.png)no-repeat;background-size: 1149rpx 187rpx;margin: 53rpx auto 26rpx;}
.more_trs{transform: rotateZ(180deg);}
.m_special{background-position: -32rpx -60rpx;}
.m_info{background-position: -311rpx -60rpx;}
.m_rules{background-position: -592rpx -60rpx;}
.m_route{background-position: -871rpx -60rpx;}
.more{justify-content: center;color: #888888;font-size: 28rpx;height: 80rpx;line-height: 80rpx;border-bottom: 2rpx solid #d9d9d9;}
.more view:nth-child(2){width: 24rpx;height: 15rpx;background: url(http://weixin.zx-tour.com/static/xcx_img/more_icon.png) no-repeat;background-size: 100%;margin-top: 34rpx;margin-left: 8rpx;}

.introduce_box image{width: 100%!important;}
/*////////////////重要提示样式//////////////////*/
.notice_mask{position: fixed;width: 100%;height: 100%;background: rgba(0, 0, 0, 0.5);padding: 20rpx;top: 0;left: 0;z-index: 100;box-sizing: border-box;}
.notice{width: 100%;background: #ffffff;padding: 0 34rpx;box-sizing: border-box;border-radius: 10rpx;}
.notice_header{position: relative;padding-top: 18rpx;padding-bottom: 10rpx;}
.notice_line{background: #0398ff;
    height: 2px;
    width: 232rpx;
    position: absolute;
    left: 50%;
    top: 50%;
    -webkit-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%);
    z-index: 10;
    -ms-transform: translate(-50%,-50%);
    -moz-transform: translate(-50%,-50%);}
.notice_text{    width: 160rpx;
    margin: 0 auto;
    text-align: center;
    background: #FFFFFF;
    position: relative;
    font-size: 30rpx;
    z-index: 10;}
.notice_close{    position: absolute;
    top: 10px;
    right: 0;
    z-index: 1000;
    width: 40rpx;
    height: 40rpx;
    background: url(http://weixin.zx-tour.com/static/images/alert_info_colse.png) no-repeat;
    background-size: 100%;}
    /*/////////////////标签///////////////////////////*/
    .tag{position: absolute;right: 0;bottom: 93rpx;margin-right: 30rpx;}
    .WMM{background: url(http://weixin.zx-tour.com/static/xcx_img/rz_tag.png) no-repeat;background-size: 469rpx 61rpx;width: 70rpx;height: 32rpx;background-position: -314rpx -17rpx;}
    .CSA{background: url(http://weixin.zx-tour.com/static/xcx_img/rz_tag.png) no-repeat;background-size: 469rpx 61rpx;width: 70rpx;height: 32rpx;background-position: -22rpx -17rpx;}
    .UTMB{background: url(http://weixin.zx-tour.com/static/xcx_img/rz_tag.png) no-repeat;background-size: 469rpx 61rpx;width: 70rpx;height: 32rpx;background-position: -165rpx -17rpx;}
    .I-TRA{background: url(http://weixin.zx-tour.com/static/xcx_img/rz_tag.png) no-repeat;background-size: 469rpx 61rpx;width: 70rpx;height: 32rpx;background-position: -235rpx -17rpx;}
    .ISF{background: url(http://weixin.zx-tour.com/static/xcx_img/rz_tag.png) no-repeat;background-size: 469rpx 61rpx;width: 70rpx;height: 32rpx;background-position: -94rpx -17rpx;}
    .AIMS{background: url(http://weixin.zx-tour.com/static/xcx_img/rz_tag.png) no-repeat;background-size: 469rpx 61rpx;width: 70rpx;height: 32rpx;background-position: -387rpx -17rpx;}
.IAAF_G{background: url(http://weixin.zx-tour.com/static/xcx_img/rz_tag_2.png) no-repeat;background-size: 472rpx 83rpx;width: 70rpx;height: 32rpx;background-position: -162rpx -25rpx;}
.CAA_G{background: url(http://weixin.zx-tour.com/static/xcx_img/rz_tag_2.png) no-repeat;background-size: 472rpx 83rpx;width: 70rpx;height: 32rpx;background-position: -386rpx -25rpx;}
.IAAF_G{background: url(http://weixin.zx-tour.com/static/xcx_img/rz_tag_2.png) no-repeat;background-size: 472rpx 83rpx;width: 70rpx;height: 32rpx;background-position: -162rpx -25rpx;}
.IAAF_G{background: url(http://weixin.zx-tour.com/static/xcx_img/rz_tag_2.png) no-repeat;background-size: 472rpx 83rpx;width: 70rpx;height: 32rpx;background-position: -162rpx -25rpx;}
.IAAF_G{background: url(http://weixin.zx-tour.com/static/xcx_img/rz_tag_2.png) no-repeat;background-size: 472rpx 83rpx;width: 70rpx;height: 32rpx;background-position: -162rpx -25rpx;}
.IAAF_G{background: url(http://weixin.zx-tour.com/static/xcx_img/rz_tag_2.png) no-repeat;background-size: 472rpx 83rpx;width: 70rpx;height: 32rpx;background-position: -162rpx -25rpx;}
.IAAF_G{background: url(http://weixin.zx-tour.com/static/xcx_img/rz_tag_2.png) no-repeat;background-size: 472rpx 83rpx;width: 70rpx;height: 32rpx;background-position: -162rpx -25rpx;}
.rz_log{width: 120rpx;height: 36rpx;background: url(http://weixin.zx-tour.com/static/xcx_img/zx_rz.png) no-repeat;background-size: 100%;}
/*//////报名提醒///////*/
.attend_mask{position: fixed;z-index: 100;top: 0;left: 0;width: 100%;height: 100%;background: rgba(0, 0, 0, 0.5);}
.attend_content{box-sizing: border-box;
    width: 640rpx;
    position: fixed;
    top: 30%;
    left: 50%;
    background: #FFFFFF;
    border-radius: 12rpx;
    transform: translateX(-50%);
    -webkit-transform: translateX(-50%);
    -moz-transform: translateX(-50%);
    -ms-transform: translateX(-50%);
    }
.attend_title{font-size: 34rpx;margin-bottom: 24rpx;}
.attend_select{font-size: 24rpx;margin-bottom: 24rpx;height: 24rpx;line-height: 24rpx;}
.attend_btn{width: 100%;border-top: 2rpx solid #d5d5d6;height: 82rpx;line-height: 82rpx;color: #0398ff;font-size: 30rpx;}
.attend_btn view:first-child{flex: 1;text-align: center;border-right: 2rpx solid #d5d5d6;}
.attend_btn view:last-child{flex: 1;text-align: center;}