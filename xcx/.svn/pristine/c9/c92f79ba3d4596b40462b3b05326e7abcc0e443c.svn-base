/**app.wxss**/
@import 'style/weui.wxss';
page{height: 100%;}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.flex-wrp{
    display:flex;
    flex-direction: row;
}
.footer{height: 98rpx;width: 100%;background: #ffffff;position: fixed;bottom: 0;left: 0;border-top: 2rpx solid #eaeaea;box-sizing: border-box;}
.foot_conmon{flex: 1;text-align: center;font-size: 22rpx;}
.foot_conmon view:nth-child(1){background: url(http://weixin.zx-tour.com/static/xcx_img/nav_icon.png) no-repeat;background-size: 421rpx 63rpx;margin-top: 14rpx;}
.foot_conmon view:nth-child(2){line-height: 22rpx;color: #808080;}
.match{width: 48rpx;height: 44rpx;margin: 0 auto 0;background-position: -10rpx -10rpx!important;margin-bottom: 11rpx;}
.order{width: 45rpx;height: 43rpx;margin: 0 auto 0;background-position: -231rpx -10rpx!important;margin-bottom: 13rpx;}
.info{width: 42rpx;height: 46rpx;margin: 0 auto 0;background-position: -370rpx -10rpx!important;margin-bottom: 9rpx;}
