<view class="race_name">{{racemsg[0].m_name}}</view>
<block wx:for="{{field}}" wx:for-index="_index">
	<view class="runner_num_wrap">
		<view class="back_line">
			<view class="runner_num">参赛者#<text>{{_index+1}}</text></view>
		</view>
	</view>
	<view class="base_info">
		<block wx:for="{{field[_index]}}" wx:for-key="order" wx:for-index="field_index">
			<view wx:if="{{item.isshow == '1' && field_index != 'proveimg' && field_index != 'healthyimg' && field_index != 'sexy' && field_index != 'isattended' && field_index != 'personbest' && field_index != 'personbestmatch' }}" class="show_msg">
				<view class="show_name">{{item.name}}</view>
				<view class="real_data">{{usermsg[_index][field_index]}}</view>
			</view>
			<view wx:if="{{item.isshow == '1' && field_index == 'sexy'}}" class="show_msg">
				<view class="show_name">{{item.name}}</view>
				<view wx:if="{{usermsg[_index][field_index] == '1'}}" class="real_data">男</view>
				<view wx:else class="real_data">女</view>
			</view>
			<view wx:if="{{item.isshow == '1' && field_index == 'isattended'}}" class="show_msg">
				<view class="show_name">{{item.name}}</view>
				<view wx:if="{{usermsg[_index][field_index] == '0'}}" class="real_data">否</view>
				<view wx:else class="real_data">是</view>
			</view>

			<view wx:if="{{item.isshow == '1' && field_index == 'personbest' && usermsg[_index].isattended == '1'}}" class="show_msg">
				<view class="show_name">{{item.name}}</view>
				<view class="real_data">{{usermsg[_index][field_index]}}</view>
			</view>
			<view wx:if="{{item.isshow == '1' && field_index == 'personbestmatch' && usermsg[_index].isattended == '1'}}" class="show_msg">
				<view class="show_name">{{item.name}}</view>
				<view class="real_data">{{usermsg[_index][field_index]}}</view>
			</view>

			<view class="img_wrap" wx:if="{{item.isshow == '1' && (field_index == 'proveimg' || field_index == 'healthyimg')}}">
				<image class="img" src='{{usermsg[_index][field_index]?usermsg[_index][field_index]:no_imgsrc}}' style="{{usermsg[_index][field_index]?0:no_imgsize}}"></image>
				<view class="img_name">{{item.name}}</view>
			</view>
		</block>	
	</view>
</block>
<navigator url="orderinfo?orderid={{usermsg[0].orderid}}">
	<button type="primary" class="go_back_btn">返回订单详情</button>
</navigator>