page{
    background: rgb(243, 246, 245);
}
.head_wrap {
  width: 100%;
  height: 168rpx;
  text-align: center;
}

.head_img {
  width: 690rpx;
  height: 100rpx;
  margin-top: 36rpx;
}
.success_desc{
    background: white;
    width: 100%;
    box-sizing: border-box;
    padding: 20rpx 40rpx;
    font-size: 28rpx;
    text-align: center;
    color: #888;
    line-height: 54rpx;
}
.success_code_wrap{
    background: white;
    width: 100%;
    height: 440rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
.success_code{
    width: 440rpx;
    height: 440rpx;
}
.code_des{
    width: 100%;
    text-align: center;
    line-height: 50rpx;
    font-size: 32rpx;
    color: #888;
    background: white;
}
.success_btn_wrap{
    width: 100%;
    background: white;
    padding: 40rpx 0 80rpx 0;
    box-sizing: border-box;
}
.go_orderinfo{
    background: #0398ff!important;
    width: 634rpx;
    height: 88rpx;
    line-height: 88rpx;
    font-size: 30rpx;
}
.lack_des{
    width: 100%;
    padding: 0 10rpx;
    box-sizing: border-box;
    font-size: 34rpx;
    color: #3e3e3e;
    line-height: 58rpx;
    text-align: center;
}
.lack_btn_wrap{
    box-sizing: border-box;
    padding: 0 20rpx;
    width: 100%;
    display: flex;
    justify-content: space-around; 
    flex-direction: row;
    margin-top:100rpx;
}
.lack_btn{
    width: 320rpx;
    height: 88rpx;
    line-height: 88rpx;
    font-size: 34rpx;
    background: #0398ff!important;
    border-radius: 6rpx;
}
.later_finish{
    background: white!important;
    border: 2rpx solid #e8e8e8;
    color: #0398ff!important;
}
.lack_wrap{
    background: white;
    box-sizing: border-box;
    padding-top: 100rpx;
    padding-bottom: 60rpx;
}
.spc_color{
    color: #0398ff;
}