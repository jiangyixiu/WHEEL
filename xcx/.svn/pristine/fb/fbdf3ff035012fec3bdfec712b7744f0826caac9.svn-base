page{
    background: #f4f4f4;
}
.head_wrap{
    padding:0.1px 19rpx;
    box-sizing: border-box;
    width: 100%;
    height: 150rpx;
    position: relative;
    background-size: cover;
}
.head_title{
    font-size: 40rpx;
    color: white;
    float: left;
    margin: 43rpx 0 0 20rpx;
}
.no_pay_title{
     margin: 20rpx 0 0 20rpx;
}
.count_time{
    font-size: 24rpx;
    color: white;
    position: absolute;
    left: 140rpx;
    top: 85rpx;
}
.head_righticon{
    width: 50rpx;
    height: 52rpx;
    float: left;
    margin:50rpx 0 0 50rpx;
    background: url('http://weixin.zx-tour.com/static/xcx_img/orderinfo_headicon.png')no-repeat;
    background-size: 320rpx 70rpx;
    background-position: 0 -10rpx;
}
.no_perfecticon{
    background-position: -70rpx -10rpx;
}
.no_pay{
    background-position: -140rpx -10rpx;
}
.over_pay{
    background-position: -206rpx -6rpx;
}
.race_wrap{
    width: 100%;
    height: 156rpx;
    box-sizing: border-box;
    padding:0.1px 18rpx;
    background: white;
}
.img_info_wrap{
    width: 100%;
    height: 156rpx;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
}
.race_img{
    width: 180rpx;
    height: 115rpx;
    margin-top: 20rpx;    
}
.info_wrap{
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    max-width: 520rpx;
    box-sizing: border-box;
    margin-top: 10rpx;
    padding-left: 20rpx;
}
.race_info{
    color: rgb(171,171,171);
    font-size: 28rpx;
    max-width: 520rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    line-height: 45rpx;
}
.race_name{
    color: #3e3e3e;
    font-weight: bold;
}
.race_foot{
    width: 100%;
    height: 96rpx;
    line-height: 96rpx;
    position: fixed;
    bottom: 0;
    left: 0;
    box-sizing: border-box;
    padding: 0 15rpx;
    background:#fafafa;
    border-top: 0.5rpx solid #e8e8e8;
    z-index: 999;
}
.race_statu{
    width: 180rpx;
    line-height: 85rpx;
    float: right;
    font-size: 28rpx;
    color: white;
    text-align: center;
    margin-top: 33rpx;
}
.all_bt{
    float: right;
    padding:0 15rpx;
    height: 70rpx;
    line-height: 70rpx;
    margin-top: 15rpx;
    background: #0398ff!important;
    font-size:26rpx; 
}
.already_wrap{
    margin-top: 20rpx;
    width: 100%;
    height: 230rpx;
    padding:0.1px 15rpx;
    box-sizing: border-box; 
    background: white;
}
.already_info{
    width: 100%;
    height: 220rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    line-height: 60rpx;
    box-sizing: border-box;
    padding:10rpx 0; 
}
.alreay_name{
    max-width: 710rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size:28rpx;
    color: #3e3e3e;
}
.already_money{
    box-sizing: border-box;
    width: 100%;
    height: 220rpx;
    display: flex;
    flex-direction: column;
    justify-content:center;
    line-height: 53rpx;
    background: white;
    padding-left:15rpx;
    margin-top: 20rpx;
}
.already_sign_money{
    font-size: 28rpx;
    color: #888888;
}
.already_ordermoney{
    margin-right: 15rpx;
    color: #333;
}
.money_color_red{
    color: #f11919;
    font-size: 30rpx;
}
.already_copmoney{
    color: #333;
    margin-right: 15rpx;
}
.real_show_money{
    float: right
}
.already_paymoney{
    border-top: 0.5rpx solid #e8e8e8;
    text-align: right;
    font-size: 28rpx;
    color: #3e3e3e;
    margin-right: 15rpx;
    padding-right: 15rpx;
    width: 100%;
    box-sizing: border-box;
    margin-top: 15rpx;
    padding-top: 15rpx; 
}
.go_namelist{
    background: white;
    margin-top: 20rpx;
    width: 100%;
    height: 88rpx;
    display: flex;
    justify-content: space-between;
    line-height: 88rpx;
    box-sizing: border-box;
    padding: 0 15rpx;
    font-size: 28rpx;
    color: #3e3e3e;
}
.more_title{
    color: #888888;
}
.more_left{
    margin-left: 25rpx;
}
.qa_phone_wrap{
    display: flex;
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    font-size: 28rpx;
    color: #3e3e3e;
    background: white;
    margin-top: 20rpx;
    box-sizing: border-box;
    flex-direction: row-reverse;
}
.qa_phone_wrap_bottom_wrap{
    height: 200rpx;
}
.qa_phone{
    height: 52rpx;
    line-height: 52rpx;
    padding: 0 15rpx;
    background: #ff9320!important;
    float: right;
    margin: 17rpx 18rpx 0 32rpx;
    font-size: 26rpx; 
}
.orderinfo_foot{
    box-sizing: border-box;
    padding: 15rpx 0 15rpx 15rpx;
    font-size: 24rpx;
    color: #888888;
    line-height: 45rpx;
    background: white;
    margin-top: 20rpx;
}
button{
    border: none
}
