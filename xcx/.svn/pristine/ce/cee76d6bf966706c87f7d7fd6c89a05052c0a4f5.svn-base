.wrap{height: 100%;width: 100%;background: #f6f6f6;box-sizing: border-box;padding: 0 20rpx;overflow: hidden;font-family: '微软雅黑','Microsoft YaHei';}
.conmon{height: 336rpx;width: 340rpx;float: left;background: #ffffff;text-align: center;flex: 1;margin-top: 30rpx;box-shadow: 0 0 15rpx rgba(206, 206, 206, 0.2)}
.conmon view:nth-child(1){background: url(http://weixin.zx-tour.com/static/xcx_img/index_icon.png) no-repeat;background-size: 989rpx 214rpx;width: 154rpx;height: 152rpx;margin: 58rpx auto 37rpx;}
.conmon view:nth-child(2){font-size: 34rpx;line-height: 34rpx;color: #666666;}
.broad{background-position: -39rpx -36rpx!important;}
.china{background-position: -229rpx -36rpx!important;}
.malsong{background-position: -420rpx -36rpx!important;}
.yueye{background-position: -608rpx -36rpx!important;}
.huanle{background-position: -798rpx -36rpx!important;}
/*///////////////////////底部导航条//////////////////*/
.footer{height: 98rpx;width: 100%;background: #ffffff;position: fixed;bottom: 0;left: 0;border-top: 2rpx solid #eaeaea;}
.foot_conmon{flex: 1;text-align: center;font-size: 22rpx;}
.foot_conmon view:nth-child(1){background: url(http://weixin.zx-tour.com/static/xcx_img/nav_icon.png) no-repeat;background-size: 421rpx 63rpx;margin-top: 14rpx;}
.foot_conmon view:nth-child(2){line-height: 22rpx;color: #808080;}
.match{width: 48rpx;height: 44rpx;margin: 0 auto 0;background-position: -10rpx -10rpx!important;margin-bottom: 11rpx;}
.order{width: 45rpx;height: 43rpx;margin: 0 auto 0;background-position: -231rpx -10rpx!important;margin-bottom: 13rpx;}
.info{width: 42rpx;height: 46rpx;margin: 0 auto 0;background-position: -370rpx -10rpx!important;margin-bottom: 9rpx;}
/*/////////////////底部付款//////////////////*/
.footer{width: 100%;box-sizing: border-box;position: fixed;left: 0;bottom: 0;}