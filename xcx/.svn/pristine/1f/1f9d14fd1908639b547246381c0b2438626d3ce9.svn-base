page{
  background: #f6f6f6;
  font-family: 'Microsoft Arial','微软雅黑';
}
.container{
  margin: 0;
  padding: 0;
}
.head_wrap{
    width: 100%;
    height: 560rpx;
    padding: 0.1rpx;
}
.img_wrap{
  margin-top: 100rpx;
  width: 100%;
  height: 100%;
  text-align: center;
}
.head_img{
  width: 250rpx;
  height: 200rpx;
}
.head_title{
  margin-top: 40rpx;
  width: 100%;
  text-align: center;
  font-size: 35rpx;
  color:#0398ff;
}
.btn_wrap{
  width: 100%;
  height: 100rpx;
  text-align: center;
  position: relative;
}
.go_index{
  width: 500rpx;
  height: 94rpx;
  background: #0398ff!important;
  line-height: 94rpx;
  border-radius: 48rpx;
}
.tab_head{
  background: white;
  width: 100%;
  height: 88rpx;
  font-size: 34rpx;
  display: flex;
  flex-direction: row;
}
.order_tap{
  flex: 1;
  text-align: center;
  line-height: 100rpx;
  color: #333;
}
#order_tap_show{
  border-bottom: 7rpx solid #0398ff;
  color: #0398ff;
}
button{
    border-radius: 4px!important;
}
button::after{
    border: none!important;
}
.list_wrap{
  margin-top: 20rpx;
  width: 100%;
  height: 325rpx;
  background: #f7f7f7;
}
.list_head{
  width: 100%;
  height: 78rpx;
  line-height: 78rpx;
  font-size: 32rpx;
  box-sizing: border-box;
  padding-left: 20rpx;
  padding-right: 20rpx;
  background: white;
}
.list_type{
  margin-top: 22rpx;
  width: 35rpx;
  height: 35rpx;
  font-size: 20rpx;
  background:#0398ff;
  float: left;
  border-radius: 50%;
  text-align: center;
  line-height: 35rpx;
  color: white;
}
.list_name{
  margin-left: 10rpx;
  float: left;
  max-width: 500rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-weight: bold;
}
.list_statu{
  font-size: 24rpx;
  color: #ff7400;
  float: right;
}
.list_statu_success{
  color: #0398ff;
}
.img_intr_wrap{
  box-sizing: border-box;
  padding-left: 20rpx;
  padding-right: 20rpx;
  width: 100%;
  height: 158rpx;
  display: flex;
  flex-direction: row;
}
.list_img{
  width: 179rpx;
  height: 116rpx;
  margin-top: 20rpx;
}
.intr_wrap{
  box-sizing: border-box;
  padding-top: 14rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  font-size: 28rpx;
  line-height: 45rpx;
  margin-left: 20rpx;
  color: #888888;
}
.list_intrname{
 max-width: 520rpx;
 overflow: hidden;
 text-overflow: ellipsis;
 white-space: nowrap;
}
.list_foot{
  box-sizing: border-box;
  padding-left: 20rpx;
  padding-right: 20rpx;
  width: 100%;
  height:88rpx;
  line-height: 88rpx;
  background: white;
}
.list_needpay{
  float: left;
  font-size: 28rpx;
  color: #888888;
}
.list_realmoneyicon{
  display: inline-block;
  color: #ff7400;
}
.list_pre{
  margin-top: 28rpx;
  width: 31rpx;
  height: 37rpx;
  background: #ffd800;
  position: relative;
  text-align: center;
  float: left;
  line-height: 37rpx;
  margin-left: 20rpx;
  font-size: 22rpx;
}
.list_pre::after{
  content: "";
  width: 0;
  height: 0;
  border: 14rpx solid transparent;
  border-bottom: 10rpx solid white;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  margin: 0 auto;
}
.race_statu{
  float: right;
  font-size: 28rpx;
}
.list_btn{
  height: 50rpx;
  line-height: 50rpx;
  float: right;
  margin-left: 15rpx;
  font-size: 26rpx;
  padding: 0;
  text-align: center;
  margin-top: 18rpx;
  background: #0398ff!important;
  width: 140rpx;
  box-sizing: border-box;
}
.btn_gray{
  background:rgb(171,171,171)!important;
}
.btn_orange{
  background:#ff7400!important;
}
.nopayorder_hidden{
  display: none;
}
.noperfect_hidden{
  display: none;
}
.nopay_orange{
  color: #ff7400;
}
.no_sign_img{
    width: 120rpx;
    height: 120rpx;
    margin:300rpx 0 0 314rpx;
}
.no_sign_title{
    font-size: 36rpx;
    width: 100%;
    text-align: center;
    color: #999;
    margin-top: 30rpx;
    margin-bottom: 40rpx;
}
.go_landing{
    width: 300rpx;
    height: 88rpx;
    line-height: 88rpx;
    background:#0398ff;
    color: white;
    font-size: 36rpx;
    font-weight: bold;
    border-radius: 44rpx;
    text-align: center;
    margin-left: 224rpx;
}
.white_wrap{
    width: 100%;
    height: 100%;
    position: fixed;
    background: white;
    z-index: 99;
    top: 0;
    left: 0;
}