.head_wrap{
    width: 100%;
    height: 500rpx;
    background: url('http://weixin.zx-tour.com/static/images/ht_images/Ht_activityHead.jpg')no-repeat left center;
    background-size: contain;
    position: relative;
    overflow: hidden;
}
.go_ht{
    width: 250rpx;
    height: 50rpx;
    background: rgba(0, 0, 0, 0.45);
    color: white;
    font-size: 28rpx;
    line-height: 50rpx;
    border-radius: 25rpx;
    text-indent: 28rpx;
    position: absolute;
    right: -220rpx;
    bottom: 70rpx;
    animation: show_ht 2s ease 0.5s forwards;-webkit-animation: show_ht 2s ease 0.5s forwards;
}
.go_intr{
    width: 250rpx;
    height: 50rpx;
    background: rgba(0, 0, 0, 0.45);
    color: white;
    font-size: 28rpx;
    line-height: 50rpx;
    border-radius: 25rpx;
    text-indent: 36rpx;
    transform: scale(0.7);
    -webkit-transform: scale(0.7);
    position: absolute;
    right: -190rpx;
    bottom: 15rpx;
    animation: show_intr 2s ease 0.5s forwards;-webkit-animation: show_intr 2s ease 0.5s forwards;
}
@keyframes show_intr{
    from{right: -190rpx;}
    to{right: -80rpx;}
} 
@-webkit-keyframes show_intr{
    from{right: -190rpx;}
    to{right: -80rpx;}
}
@keyframes show_ht{
    from{right: -220rpx;}
    to{right: -40rpx;}
}
@-webkit-keyframes show_ht{
    from{right: -220rpx;}
    to{right: -40rpx;}
}
.head_intr{
    width: 710rpx;
    height: 696rpx;
    border: 1rpx solid rgb(143, 142, 142);
    margin-left: 18rpx;
    background: url('http://weixin.zx-tour.com/static/images/ht_images/Ht_timeBg1.jpg')no-repeat;
    background-size: contain;
    margin-bottom: 30rpx;
}
.head_msg_wrap{
    margin-top: 35rpx;
}
.head_msg{
    text-align: center;
    font-size: 24rpx;
    line-height: 45rpx;
}
.msg_spc_color{
    color: rgb(163, 26, 142);
}
.head_code_wrap{
    margin-top: 10rpx;
}
.head_code{
    width: 100%;
    text-align: center;
    height: 231rpx;
    margin-top: 10rpx;
}
.code{
    width: 231rpx;
    height: 231rpx;
}
.input_title{
    width: 100%;
    font-size: 30rpx;
    font-weight: bold;
    text-align: center;
    margin-top: 10rpx;
}
.input_msg{
    margin-top: 20rpx;
    width: 100%;
    display: flex;
}
.input_all{
    border: 1rpx solid rgba(128, 124, 126, 0.3);
    border-radius: 10rpx;
    font-size: 28rpx;
}
.input_all:hover{
    border-color: #ffca03;
}
.input_name{
    width: 160rpx;
    margin-left: 50rpx;
}
.input_phone{
    width: 265rpx;
    margin-left: 20rpx;
}
.btn_tj{
    width: 140rpx;
    height: 50rpx;
    font-size: 28rpx;
    background: #ffca03!important;
    padding: 0;
    line-height: 50rpx;
}
.race_wrap{
    width: 100%;
    position: relative;
    margin-bottom: 25rpx;
    border-bottom: 5rpx solid #f2f3f4;
    box-sizing: border-box;
    padding-bottom: 10rpx;
}
.race_img{
    width: 100%;
    height: 300rpx;
    background:url('http://weixin.zx-tour.com/static/images/ht_images/mgw.png')no-repeat;
    background-size: cover;
    background-position: 0% -170rpx;
}
.race_img_ylsl{
     background:url('http://weixin.zx-tour.com/static/images/ht_images/ylsl.jpg')no-repeat;
    background-size: cover;
    background-position: 0 top;
}
.race_img_zxpr{
    background: url('http://weixin.zx-tour.com/static/images/ht_images/cx.jpg')no-repeat;
    background-size: cover;
}
.race_img_bl{
    background: url('http://weixin.zx-tour.com/static/images/ht_images/bl.png')no-repeat;
    background-size: cover;
    background-position: 0 50%;
}
.race_img_zjg{
    background: url('http://weixin.zx-tour.com/static/images/ht_images/zjg.jpg')no-repeat;
    background-size: cover;
    background-position: 0 top;
}
.race_place{
    font-size: 24rpx;
    color: black;
    background: #ffd800;
    padding-left: 10rpx;
    padding-right: 10rpx;
