    box-sizing: border-box;
    padding-left: 15rpx;
    padding-right: 15rpx;
    height: 150rpx;
    background: white;
}
.race_name{
    margin-top: 20rpx;
    font-weight: bold;
    font-size: 30rpx;
    color: black;
}
.race_des{
    white-space: nowrap;
    font-size: 28rpx;
    color: rgb(171, 171, 171);
}
.race_time_money{
    width: 100%;
