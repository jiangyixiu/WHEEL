.flex-wrp{flex-direction: row;}
.flex-item{flex: 1;}
.wrap{font-family: '微软雅黑','Microsoft YaHei';min-height: 100%;background: #f6f6f6;overflow: hidden;box-sizing: border-box;position: relative;}
.search{height: 88rpx;line-height: 88rpx;border-bottom: 2rpx solid #f6f6f6;position: fixed;top: 0;left: 0;width: 100%;background: #ffffff;z-index: 100;}
.line{width: 1px;height: 50rpx;background: #dbdbdb;margin-top: 19rpx;}
.search_title{text-align: center;}

.search_bar{width: 178rpx;box-sizing: border-box;padding: 0 5rpx;}
.search_bar>view:nth-child(1){height: 100%;flex: 1;}
.run_search_icon{width: 30rpx;height: 30rpx;background: url(http://weixin.zx-tour.com/static/xcx_img/run_search_icon.png) no-repeat;background-size: 100%;margin-top: 29rpx;margin-left: 39rpx;}
.search_bar>view:nth-child(2){height: 100%;flex: 1;}
.run_qiehuan{width: 37rpx;height: 37rpx;background: url(http://weixin.zx-tour.com/static/xcx_img/run_qiehuan.png) no-repeat;background-size: 100%;margin-top: 26rpx;margin-left: 23rpx;}
.run_qiehuan_2{width: 43rpx;height: 37rpx;background: url(http://weixin.zx-tour.com/static/xcx_img/match_list.png) no-repeat;background-size: 100%;margin-top: 26rpx;margin-left: 23rpx;}
.conmon_title{font-size: 32rpx;justify-content: center;color: #333333}
/*/////////下拉框样式///////////*/
.drop_content{
              position: fixed; 
              top: 90rpx;
              left: 0rpx;
              width: 100%;
              height: 100%;
              font-size: 26rpx;
              box-sizing: border-box;
              background: rgba(0, 0, 0, 0.5);
              z-index: 100;
              }
.conmon_outbox{height: 738rpx;position: relative;background: #ffffff;}
.conmon{overflow: hidden;margin: 0 15rpx;border-bottom: 1rpx solid #e7e7e7;padding-bottom: 28rpx;}
.conmon_box{float: left;width: 156rpx;height: 62rpx;box-sizing: border-box;text-align: center;line-height: 62rpx;background: #f8f8f8;border-radius: 5px;position: relative;margin: 28rpx 32rpx 0 0;}
.conmon .conmon_box:nth-child(4n){margin-right: 0rpx;}
.select_icon_1{
    box-sizing: border-box;
    position: absolute;
    line-height: 62rpx;
    height: 62rpx;
    width: 156rpx;
    top: 0px;
    left: 0px;
    background-size: 38rpx;
    background-position: 116rpx 34rpx;
    border-radius: 5px 5px 5px 5px;    
}
.select_icon{
    box-sizing: border-box;
    position: absolute;
    line-height: 62rpx;
    height: 62rpx;
    width: 156rpx;
    top: 0px;
    left: 0px;
    background: url(http://weixin.zx-tour.com/static/images/run_selected.png) no-repeat;
    background-size: 38rpx;
    background-position: 116rpx 34rpx;
    border-radius: 5px 5px 5px 5px;
    border: 1px solid #ee7c2f;
    }
.type_btn{font-size: 32rpx;position: absolute;bottom: 1rpx;left: 0;width: 100%;}
.reset{height: 98rpx;border-top: 1rpx solid #dddddd;line-height: 96rpx;border-bottom: 1rpx solid #dddddd;box-sizing: border-box;}
.success{height: 98rpx;line-height: 98rpx;color: #ffffff;background: #ff7239;}
/*/////////////////////////////列表样式///*/
.search_result{background: #f6f6f6;width: 100%;min-height: 100%;padding-top: 158rpx;padding-bottom: 20rpx;}
.search_result_out{width: 100%;box-sizing: border-box;padding: 21rpx 8rpx 0 8rpx;}
.box{box-sizing: border-box;width: 100%;background: #ffffff;margin-top: 20rpx;box-shadow: 0px 1px 10px rgba(120,120,120,0.3);
    -webkit-box-shadow: 0px 1px 8px rgba(120,120,120,0.3);
    -ms-box-shadow: 0px 1px 8px rgba(120,120,120,0.3);
    -moz-box-shadow: 0px 1px 8px rgba(120,120,120,0.3);}
.box_top{background: rgba(3,152,255,0.1);height: 40rpx;overflow: hidden;font-size: 26rpx;color: #3e3e3e;}
.box_top_time{float: left;width: 188rpx;line-height: 40rpx;text-align: center;height: 40rpx;background: rgba(3,152,255,0.6);font-size: 30rpx;color: #ffffff;}
.box_top_city{float: right;height: 40rpx;line-height: 40rpx;margin-right: 14rpx;}
.box_center{overflow: hidden;padding: 12rpx 10rpx;}
.box_center_right{position: relative;width: 178rpx;height: 116rpx;overflow: hidden;}
.box_center_right .images{width: 178rpx;height: 116rpx;overflow: hidden;}
.box_center_right .images image{height: 100%;width: 100%;}
.mask{position: absolute;width: 100%;height: 100%;background: rgba(0, 0, 0, 0.35);top: 0;left: 0;z-index: 5;line-height: 116rpx;text-align: center;color: #ffffff;font-size: 28rpx;}
.tag{position: absolute;width: 170rpx;height: 44rpx;    font-size: 24rpx;
    color: #FFFFFF;
    background: #ff9320;
    top: 8rpx;
    left: -56rpx;
    transform: scale(0.666) rotateZ(-45deg);
    -moz-transform: scale(0.666) rotateZ(-45deg);
    -ms-transform: scale(0.666) rotateZ(-45deg);
    -webkit-transform: scale(0.666) rotateZ(-45deg);
    z-index: 10;
    text-align: center;
    }
.box_center_left{flex: 1; margin-left: 25rpx;overflow: hidden;border-bottom: 2rpx solid #e8e8e8;}
.box_center_title{font-size: 32rpx;width: 100%;overflow: hidden;max-height: 84rpx;text-overflow: ellipsis;white-space: nowrap;}
.box_center_des{font-size: 26rpx;color: #888888;text-overflow: ellipsis;overflow: hidden;white-space: nowrap;}
.dandu{height: 90rpx;line-height: 90rpx;box-sizing: border-box;padding: 0 12rpx;font-size: 26rpx;color: #888888;}
.dandu_title view:nth-child(1){width: 9rpx;height: 9rpx;border-radius: 50%;background: #888888;margin: 41rpx 12rpx 0 4rpx;}
.dandu_price{flex: 1;color: #ff7400;margin-left: 38rpx;}
.dandu_price view:nth-child(1){font-size: 24rpx;margin-right: -2rpx;margin-top: 4rpx;}
.dandu_price view:nth-child(2){font-size: 34rpx;font-weight: bold;}
.dandu_price view:nth-child(3){font-size: 24rpx;margin-top: 4rpx;}
.dandu_tag{height: 34rpx;width: 92rpx;text-align: center;line-height: 34rpx;font-size: 20rpx;background: #ff7400;color: #ffffff;border-radius: 4rpx;margin-top: 29rpx;}
/*/////////////////////列表2样式//////////////*/
.box_2{box-shadow: 0px 2px 8px rgba(120,120,120,0.1);-webkit-box-shadow: 0px 2px 8px rgba(120,120,120,0.1);-ms-box-shadow: 0px 2px 8px rgba(120,120,120,0.1);-moz-box-shadow: 0px 2px 8px rgba(120,120,120,0.1);}
.box_2_top{position: relative; }
.box2_img{width: 100%;height: 321rpx;}
.box2_img image{width: 100%;height: 321rpx;}
.box2_mask{position: absolute;width: 100%;height: 100%;background: rgba(0, 0, 0, 0.35);top: 0;left: 0;}
.box2_title{font-size: 52rpx;color: #ffffff;width: 100%;box-sizing: border-box;text-align: center;font-weight: bold;position: absolute;top: 50%;left: 0;transform: translateY(-50%);padding: 0 72rpx;}
.box2_time{position: absolute;bottom: 16rpx;left: 21rpx;}
.box2_time view:nth-child(1){width: 27rpx;height: 24rpx;background: url(http://weixin.zx-tour.com/static/xcx_img/list_day_icon.png) no-repeat;background-size: 100%;}
.box2_time view:nth-child(2){font-size: 24rpx;color: #ffffff;line-height: 24rpx;margin-left: 10rpx;}
.box2_city{position: absolute;right: 0;bottom: 16rpx;color: #ffffff;font-size: 24rpx;line-height: 24rpx;}
.box2_city view:nth-child(1){width: 18rpx;height: 24rpx;background: url(http://weixin.zx-tour.com/static/xcx_img/pos_city_icon.png) no-repeat;background-size: 100%;}
.box2_city view:nth-child(2){margin: 0 24rpx 0 9rpx;}
.box2_bottom{font-size: 24rpx;color: #353535;padding: 0 21rpx;background: #ffffff;}
.day{height: 34rpx;border-radius: 2rpx;background: #333333;text-align: center;line-height: 34rpx;font-size: 20rxp;color: #ffffff;padding: 0 6rpx;}
.box2_des{line-height: 24rpx;padding-top: 17rpx;margin-bottom: 19rpx;overflow: hidden;text-overflow: ellipsis;white-space: nowrap}
.box2_info{padding-bottom: 19rpx;}
.box2_info_time{flex: 1;}
.box2_info_price{font-size: 34rpx;color: #0398ff;line-height: 34rpx;}
.box2_info_price text{font-size: 24rpx;}


/*//////////////搜索带出的条件////////////////////////////*/
.search_condition{position: fixed;top: 85rpx;left: 0rpx;height: 70rpx;width: 100%;font-size: 24rpx;line-height: 70rpx;background: #ffffff;padding-top: 5rpx;z-index: 10;}
.select_condition{position: absolute;width: 72rpx;height: 70rpx;line-height: 70rpx;text-align: right;left: 0;top: 0;color: #808080;}
.close_search_out{display: inline-block;height: 70rpx;box-sizing: border-box;padding-left: 72rpx;}
.close_search_inner{padding: 10rpx 0;height: 70rpx;box-sizing: border-box;}
.close_search{height: 40rpx;background: #f5f5f5;line-height: 40rpx;overflow: hidden;padding: 0 11rpx 0 15rpx;border-radius: 40rpx;margin-left: 20rpx;display: inline-block;box-sizing: border-box;}
.close_text{float: left;color: #666666;}
.close{width: 17rpx;height: 17rpx;float: right;margin-left: 9rpx;background: url(http://weixin.zx-tour.com/static/xcx_img/search_close.png) no-repeat;background-size: 100%;margin-top: 15rpx;}
.search_condition_line{width: 2rpx;height: 50rpx;background: #dbdbdb;margin-top: 10rpx;}
.search_condition_right{width: 99rpx;text-align: center;}
/*///////////////////*/
.run_select_icon{width: 15rpx;height: 9rpx;background: url(http://weixin.zx-tour.com/static/xcx_img/run_select_icon.png) no-repeat;background-size: 100%;margin-top: 40rpx;margin-left: 10rpx;}
.run_select_icon_1{transform: rotateZ(180deg);}
/*//////////热刺搜索页///////////////////*/
.keyward_search{height: 100%;min-height: 100%;position: relative;}
.keyward_searchbar{height: 100rpx;justify-content: center;line-height: 100rpx;background: #f0f0f0;}
.keyward_close{height: 100%;width: 128rpx;text-align: center;color: #0398ff;font-size: 30rpx;}
.keyward_mask{position: absolute;left: 0;width: 100%;height: 100%;}
.keyward_mask view{height: 88rpx;line-height: 88rpx;border-bottom: 2rpx solid #e3e3e3;color: #444444;font-size: 28rpx;padding-left: 30rpx;}
