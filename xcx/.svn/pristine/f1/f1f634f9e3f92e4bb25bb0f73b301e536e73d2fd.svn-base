/* other.wxss */
.wrap{font-family: '微软雅黑','Microsoft YaHei';min-height: 100%;background: #f6f6f6;overflow: hidden;box-sizing: border-box;position: relative;}
.title{font-size: 32rpx;line-height: 32rpx;padding: 20rpx 30rpx;}
.phone{font-size: 30rpx;line-height: 30rpx;padding: 20rpx 0;margin-left: 30rpx;border-top: 2rpx solid #d9d9d9;border-bottom: 2rpx solid #d9d9d9;}
.phone_number{color: #888888;flex: 1;}
.call{margin-right: 30rpx;color: #556F94;}
.call view:first-child{width: 26rpx;height: 26rpx;}
.info{width: 590rpx;margin: 0 auto;box-sizing: border-box;color: #888888;font-size: 28rpx;border: 2rpx solid rgba(136,136,136,0.2);border-radius: 10rpx;padding: 30rpx;}
.bank{padding: 30rpx 0 30rpx 30rpx;margin-left: 30rpx;border-top: 2rpx solid #d9d9d9;}
.bank_img{width: 144rpx;height: 126rpx;}
.bank_info{flex: 1;font-size: 28rpx;color: #888888;margin-left: 18rpx;}