page{
    background: #f4f4f4;
}
.head_wrap{
    padding:0.1px 19rpx;
    box-sizing: border-box;
    width: 100%;
    height: 150rpx;
    position: relative;
}
.head_titleicon_success{
    width: 40rpx;
    height: 40rpx;
    float: left;
    margin: 50rpx 18rpx;
    background: url('http://weixin.zx-tour.com/static/xcx_img/orderinfo_icon.png')no-repeat;
    background-size: cover;
    background-position: -43rpx 0;
}
.head_titleicon_nosuccess{
    width: 40rpx;
    height: 40rpx;
    float: left;
    margin: 50rpx 18rpx;
    background: url('http://weixin.zx-tour.com/static/xcx_img/orderinfo_icon.png')no-repeat;
    background-size: cover;
    background-position: 0 0;  
}
.head_title{
    font-size: 40rpx;
    color: white;
    float: left;
    margin: 38rpx 0;
}
.count_time{
    font-size: 24rpx;
    color: white;
    position: absolute;
    left: 100rpx;
    top: 105rpx;
}
.success_righticon{
    width: 132rpx;
    height: 90rpx;
    float: right;
    margin:35rpx 42rpx;
}
.nosuccess_righticon{
    width: 132rpx;
    height: 116rpx;
    float: right;
    margin:25rpx 42rpx;
    background: url('http://weixin.zx-tour.com/static/xcx_img/orderinfo_headicon.png')no-repeat;
    background-size: 604rpx 116rpx
}
.no_perfecticon{
    background-position: 10rpx 0;
}
.no_pay{
    background-position: -222rpx 0;
}
.over_pay{
    width: 115rpx;
    background-position: -117rpx 0;
}
.race_wrap{
    width: 100%;
    height: 260rpx;
    box-sizing: border-box;
    padding:0.1px 18rpx;
    border-bottom: 0.5rpx solid rgb(231,231,231);
    background: white;
}
.img_info_wrap{
    width: 100%;
    height: 158rpx;
    border-bottom: 0.5rpx solid rgb(231,231,231);
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
}
.race_img{
    width: 180rpx;
    height: 115rpx;
    margin-top: 20rpx;    
}
.info_wrap{
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    max-width: 520rpx;
    box-sizing: border-box;
    margin-top: 10rpx;
    padding-left: 20rpx;
}
.race_info{
    color: rgb(171,171,171);
    font-size: 28rpx;
    max-width: 520rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    line-height: 45rpx;
}
.race_name{
    color: #3e3e3e;
    font-weight: bold;
}
.race_foot{
    width: 100%;
    height: 85rpx;
    line-height: 85rpx;
}
.race_statu{
    width: 180rpx;
    line-height: 85rpx;
    float: left;
    font-size: 28rpx;
    color: #3e3e3e;
}
.all_bt{
    float: right;
    padding:0 15rpx;
    height: 50rpx;
    line-height: 50rpx;
    margin-top: 15rpx;
    background: #0398ff!important;
    font-size:26rpx; 
}
.already_wrap{
    margin-top: 30rpx;
    width: 100%;
    height: 400rpx;
    padding:0.1px 15rpx;
    box-sizing: border-box; 
    border-bottom: 0.5rpx solid rgb(231,231,231);
    border-top:0.5rpx solid rgb(231,231,231);
    background: white;
}
.already_info{
    width: 100%;
    height: 275rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    line-height: 60rpx;
    border-bottom: 0.5rpx solid rgb(231,231,231);
}
.alreay_name{
    max-width: 710rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size:28rpx;
    color: #3e3e3e;
}
.already_money{
    box-sizing: border-box;
    width: 100%;
    height: 125rpx;
    display: flex;
    flex-direction: column;
    justify-content:center;
    line-height: 53rpx;
}
.already_sign_money{
    display: flex;
    /*flex-direction: row-reverse;*/
    justify-content: flex-end;
    font-size: 28rpx;
    color: #888888;
}
.already_ordermoney{
    margin-right: 50rpx;
}
.already_paymoney{
    text-align: right;
    font-size: 28rpx;
    color: #3e3e3e;
}
.go_namelist{
    background: white;
    margin-top: 20rpx;
    width: 100%;
    height: 88rpx;
    border-top: 0.5rpx solid rgb(231,231,231);
    border-bottom: 0.5rpx solid rgb(231,231,231);
    display: flex;
    justify-content: space-between;
    line-height: 88rpx;
    box-sizing: border-box;
    padding: 0 15rpx;
    font-size: 28rpx;
    color: #3e3e3e;
}
.more_title{
    color: #888888;
}
.more_left{
    margin-left: 25rpx;
}
.qa_phone_wrap{
    display: flex;
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    font-size: 28rpx;
    color: #3e3e3e;
    border-top: 0.5rpx solid rgb(231,231,231);
    border-bottom: 0.5rpx solid rgb(231,231,231);
    background: white;
    margin-top: 20rpx;
    box-sizing: border-box;
    flex-direction: row-reverse;
}
.qa_phone{
    height: 52rpx;
    line-height: 52rpx;
    padding: 0 15rpx;
    background: #ff9320!important;
    float: right;
    margin: 17rpx 18rpx 0 32rpx;
    font-size: 26rpx; 
}
.orderinfo_foot{
    box-sizing: border-box;
    padding: 10rpx 0 0 15rpx;
    font-size: 24rpx;
    color: #888888;
    line-height: 40rpx;
}
