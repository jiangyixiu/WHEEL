page{
    background: #f6f6f6;
}
.head_wrap{
    width: 100%;
    height: 176rpx;
    box-sizing: border-box;
    padding: 14rpx 30rpx;
    background: rgb(139, 205, 250);
    display: flex;
    justify-content: space-around;
}
.text{
    width: 480rpx;
    height: 96rpx;
    margin:35rpx 24rpx;
    font-size: 26rpx;
    color: white;
    line-height: 48rpx;
}
.head_img{
    margin-top: 30rpx;
    width: 152rpx;
    height: 110rpx;
}
.import_wrap{
    width: 100%;
    height: 112rpx;
    text-align: center;
    padding: 0.1px;
    background: #f6f6f6;
}
.btn_import{
    width: 380rpx;
    height: 70rpx;
    border-radius: 35rpx;
    line-height: 70rpx;
    font-size: 26rpx;
    margin-top: 20rpx;
    background: rgb(144, 207, 44)!important;
}
button::after{
    border: none!important;
}
.msg{
    width: 100%;
    box-sizing: border-box;
    padding-left: 30rpx;
}
.input_wrap{
    width: 100%;
    height: 88rpx;
    font-size: 32rpx;
    color: #3e3e3e;
    display: flex;
    line-height: 88rpx;
    border-bottom: 0.5rpx solid rgb(217,217,217);
    justify-content: flex-start;
    align-items:center;
    box-sizing: border-box;
    padding-right: 30rpx;
}
.input_msg{
    font-size:32rpx;
    width: 480rpx;
    margin-left: 45rpx;
}
.input_name{
    width: 160rpx;
}
.go_down_icon{
    /*margin-top: rpx;btn_wrap*/
    width: 25rpx;
    height: 25rpx;
    float: right;
    margin-top: 15rpx;
    background: url('http://weixin.zx-tour.com/static/xcx_img/godown_pick.png')no-repeat;
    background-size:contain;
}
.input_wrap_twoline{
    height: 108rpx;
    line-height: 50rpx;
}
.pick_show_none{
    color: #888888;
    width: 480rpx;
    margin-left: 50rpx;
}
.pick_show{
    color: #888;
    width: 480rpx;
    margin-left: 50rpx;
}
.show_phone{
    margin-left:40rpx ;
    color:#3e3e3e;
}
.mode_select{
    width: 480rpx;
    margin-left: 50rpx;
}
.pick_mask{
    z-index: 999;
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0,0,0,0.5);
}
.finish_picker_wrap{
    width: 100%;
    position: absolute;
    bottom: 0;
    left: 0;
}
.btn_wrap{
   width: 100%;
   height: 50rpx;
   line-height: 50rpx;
   font-size: 30rpx;
   color: #888888;
   background: white;
   display: flex;
   justify-content: space-between;
   border-bottom: 0.5rpx solid #f2f3f4;
}
.btn_wrap text{
    width: 150rpx;
    text-align: center;
}
.pick_wrap{
    width: 100%;
    height: 450rpx;
    color: #888888;
    background: rgba(255,255,255,1);
    font-size: 32rpx;
}
.tj_wrap{
    padding:20rpx 0;
    background: #f6f6f6;
}
.tj_btn{
    background: #0398ff!important;
    width: 650rpx;
    height: 84rpx;
    line-height: 84rpx;
    font-size: 36rpx;
}
.bottom_view{
    width: 100%;
    height: 100rpx;    
}
.pick_sure{
    color: rgb(144, 207, 44);
}
.footer {
    z-index: 30;
}
.no_sign_img{
    width: 120rpx;
    height: 120rpx;
    margin:300rpx 0 0 314rpx;
}
.no_sign_title{
    font-size: 36rpx;
    width: 100%;
    text-align: center;
    color: #999;
    margin-top: 30rpx;
    margin-bottom: 40rpx;
}
.go_landing{
    width: 300rpx;
    height: 88rpx;
    line-height: 88rpx;
    background:#0398ff;
    color: white;
    font-size: 36rpx;
    font-weight: bold;
    border-radius: 44rpx;
    text-align: center;
    margin-left: 224rpx;
}
